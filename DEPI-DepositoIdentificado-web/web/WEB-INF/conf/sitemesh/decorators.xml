<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/pages/decorators">
	<decorator name="main" page="layout.jsp">
		<pattern>*.do</pattern>
	</decorator>

	<decorator name="componente" page="componente.jsp">
		<pattern>/filtro/*.do</pattern>
	</decorator>

	<excludes>
		<pattern>*json.do</pattern>
		<pattern>*pdf.do</pattern>
		<pattern>*excel.do</pattern>
	</excludes>
</decorators>
